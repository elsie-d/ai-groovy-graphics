<div class="row justify-content-center align-items-center" style="height: 50vh;">
    <div class="col-md-6">
        <h3>Create a New Project</h3>
        <form class="img-prompt-form">
            <div class="form-group">
                <input type="text" id="photo-input" class="form-control" placeholder="A dog holding a cat in New York">
            </div>
            <div class="form-group">
                <button type="submit" class="btn btn-primary">Generate</button>
            </div>
        </form>
        <div id="photo-container">
            <img
            src=""
            class="generated-image"
            alt=""
          />
          <br/>
          <a href="" class="download-img" target="_blank" download="generated_image">Download Image</a>
          <div class="save-btn">
                <button type="submit" class="save-btn btn-primary">Save Image</button>
            </div>
        </div>
    </div>


</div>



<script src="./js/profile.js"></script>