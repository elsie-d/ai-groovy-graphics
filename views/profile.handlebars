<div class="profile-container">
    <div class="row justify-content-center">
        <div class="col-md-6">
            <section class="new-project-section">
                <h3>Create a New Project</h3>
                <form class="img-prompt-form" onsubmit="event.preventDefault(); generatePhoto();">
                    <div class="form-group">
                        <input type="text" id="photo-input" class="form-control"
                            placeholder="A dog holding a cat in New York">
                    </div>
                    <div class="form-group">
                        <button type="submit" class="btn btn-primary">Generate</button>
                    </div>
                </form>
            </section>
            <section id="rendering-block">
                <div class="photo-container">
                    <img src="https://oaidalleapiprodscus.blob.core.windows.net/private/org-h0Ybpgm6ZP1hgPxrZinxWgpa/user-0zkfokfSvHHU6HOmzOnKvkWw/img-XMmrfjT891uXZRT93kpI3e6w.png?st=2024-01-09T03%3A23%3A14Z&se=2024-01-09T05%3A23%3A14Z&sp=r&sv=2021-08-06&sr=b&rscd=inline&rsct=image/png&skoid=6aaadede-4fb3-4698-a8f6-684d7786b067&sktid=a48cca56-e6da-484e-a814-9c849652bcb3&skt=2024-01-08T17%3A47%3A19Z&ske=2024-01-09T17%3A47%3A19Z&sks=b&skv=2021-08-06&sig=G7B2s4CyRvDFag8ZKzbNO0tciUoVSsJdGJnD6zCTdPc%3D"
                        class="generated-image" alt="" />
                </div>
                <div class="d-flex justify-content-between" id="btns-generated">
                    <div>
                        <a href="" class="download-img" target="_blank" download="generated_image">Download Image</a>
                    </div>
                    <div>
                        <button type="submit" class="save-btn btn-primary">Save Image</button>
                    </div>
                </div>
            </section>
        </div>
    </div>
    <section id="saved-imgs" class="saved-image-row">
        <h3><u>Album Creations</u></h3>
        {{#if images.length}}
        <div class="saved-row">
            {{#each images as |image|}}
            <div class="saved-image">
                <img src="{{image.url}}" id="saved-img-card" />
                <br />
                <button class="btn btn-sm del-btn" data-id="{{image.id}}">Delete</button>
            </div>
            {{/each}}
        </div>
        {{/if}}
    </section>



    <script src="./js/profile.js"></script>